filetype off

syntax enable
set number
set tabstop=4
set hlsearch
set shiftwidth=4
set expandtab
set cursorline
set autoindent
set smartindent
set incsearch
set nocompatible
set colorcolumn=100
set smartcase
set softtabstop=4
set background=dark
set laststatus=2
filetype plugin indent on
set wildignore+=*.o,.git,CMakeFiles,CMakeCache.txt
